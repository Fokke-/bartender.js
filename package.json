{
  "name": "@fokke-/bartender.js",
  "version": "3.2.0-beta.1",
  "description": "Accessible off-canvas bars",
  "author": "Ville Fokke Saarivaara",
  "license": "MIT",
  "private": false,
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/Fokke-/bartender.js"
  },
  "keywords": [
    "off-canvas",
    "accessibility",
    "a11y",
    "sidebar",
    "navigation"
  ],
  "scripts": {
    "dev": "vite --host",
    "build": "vite build",
    "lint": "eslint ./src"
  },
  "files": [
    "LICENSE",
    "README.md",
    "dist"
  ],
  "types": "./dist/index.d.ts",
  "main": "./dist/bartender.js",
  "module": "./dist/bartender.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/bartender.js",
      "require": "./dist/bartender.umd.cjs"
    },
    "./dist/bartender.css": {
      "import": "./dist/bartender.css",
      "require": "./dist/bartender.css"
    }
  },
  "devDependencies": {
    "@eslint/js": "9.28.0",
    "@nabla/vite-plugin-eslint": "2.0.5",
    "@types/node": "22.15.29",
    "eslint": "9.28.0",
    "sass": "1.89.1",
    "typescript": "5.8.3",
    "typescript-eslint": "8.33.1",
    "vite": "6.3.5",
    "vite-plugin-dts": "4.5.4"
  }
}
