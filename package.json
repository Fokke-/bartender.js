{
  "name": "@fokke-/bartender.js",
  "version": "2.0.0",
  "description": "Accessible off-canvas bars",
  "author": "Ville Fokke Saarivaara",
  "license": "MIT",
  "private": false,
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/Fokke-/bartender.js"
  },
  "keywords": [
    "off-canvas",
    "accessibility",
    "a11y"
  ],
  "scripts": {
    "dev": "vite --host",
    "build": "vite build",
    "lint": "eslint ./src"
  },
  "files": [
    "LICENSE",
    "README.md",
    "dist"
  ],
  "types": "./dist/index.d.ts",
  "main": "./dist/Bartender.umd.cjs",
  "module": "./dist/Bartender.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/Bartender.js",
      "require": "./dist/Bartender.umd.cjs"
    },
    "./dist/bartender.scss": {
      "import": "./dist/bartender.scss",
      "require": "./dist/bartender.scss"
    },
    "./dist/bartender.css": {
      "import": "./dist/bartender.css",
      "require": "./dist/bartender.css"
    }
  },
  "devDependencies": {
    "@types/node": "^18.15.11",
    "@typescript-eslint/eslint-plugin": "^5.57.0",
    "@typescript-eslint/parser": "^5.57.0",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.37.0",
    "sass": "^1.60.0",
    "stylelint": "^15.4.0",
    "stylelint-config-sass-guidelines": "^10.0.0",
    "stylelint-config-standard": "^32.0.0",
    "typescript": "^5.0.3",
    "vite": "^4.2.1",
    "vite-plugin-dts": "^2.1.0",
    "vite-plugin-eslint": "^1.8.1",
    "vite-plugin-static-copy": "^0.13.1",
    "vite-plugin-stylelint": "^4.3.0"
  },
  "dependencies": {
    "async-await-queue": "^2.1.4",
    "focus-trap": "^7.4.0",
    "ts-debounce": "^4.0.0"
  }
}
